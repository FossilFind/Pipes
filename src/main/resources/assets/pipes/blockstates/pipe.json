{
	"multipart": [
		{
			"apply": { "model": "pipes:block/pipe_core" }
		},
		{
			"when": { "north": "true" },
			"apply": { "model": "pipes:block/pipe_connection", "uvlock": false }
		},
		{
			"when": { "east": "true" },
			"apply": { "model": "pipes:block/pipe_connection", "uvlock": false, "y": 90 }
		},
		{
			"when": { "south": "true" },
			"apply": { "model": "pipes:block/pipe_connection", "uvlock": false, "y": 180 }
		},
		{
			"when": { "west": "true" },
			"apply": { "model": "pipes:block/pipe_connection", "uvlock": false, "y": 270 }
		},
		{
			"when": { "up": "true" },
			"apply": { "model": "pipes:block/pipe_connection", "uvlock": false, "x": 270 }
		},
		{
			"when": { "down": "true" },
			"apply": { "model": "pipes:block/pipe_connection", "uvlock": false, "x": 90 }
		}
	]
}